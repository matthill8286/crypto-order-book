{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "lib": [
      "es2015",
      "dom",
      "es6",
      "es2017",
      "es2019",
      "esnext.asynciterable"
    ],
    "moduleResolution": "Node",
    "allowJs": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@matthill8286/atomic-icon-library": ["node_modules/@matthill8286/atomic-icon-library/src/index.ts"]
    }
  },
  "exclude": [
    "node_modules",
    ".next",
    "out"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js"
  ]
}
